#include<iostream>
#include<cmath>
using namespace std;
int main()
{
	int a,b,c,d,e,f;
	cout<<"enter the values of a , b and c"<<endl;
	cin>>a>>b>>c;
	d=sqrt(b*b-4*a*c);
	e=-b+d/2*a;
	f=-b-d/2*a;
	cout<<"the roots of the quadratic equation are as follows "<<endl;
	cout<<e<<endl;
	cout<<f<<endl;

	return 0;
	
}
